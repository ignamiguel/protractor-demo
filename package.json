{
  "name": "protractor-demo",
  "version": "1.0.0",
  "description": "Demo project for Protractor framework",
  "scripts": {
    "skip_pretest": "webdriver-manager update",
    "test": "protractor config/protractor.config.js",
    "test:sauce": "protractor config/sauce.config.js",
    "test:runner": "node ./config/protractor-runner.js",
    "prereport": "rm -rf allure-report",
    "report": "allure generate allure-results -o allure-report && allure open allure-report",
    "clean": "rm -rf allure-report allure-results"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignamiguel/protractor-demo.git"
  },
  "keywords": [
    "protractor",
    "demo"
  ],
  "author": "ignamiguel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ignamiguel/protractor-demo/issues"
  },
  "homepage": "https://github.com/ignamiguel/protractor-demo#readme",
  "dependencies": {
    "cli-color": "^1.2.0",
    "jasmine-allure-reporter": "^1.0.1",
    "log4js": "^2.4.1",
    "pix-diff": "^2.0.1",
    "protractor": "^5.1.2",
    "sauce-connect-launcher": "^1.2.3",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "jasmine-spec-reporter": "^4.2.1"
  }
}
